{
  "name": "simpsons-names",
  "version": "0.0.0-semantically-released",
  "description": "Get random names form The Simpsons characters.",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "babel --presets es2015 src/index.js -o dist/index.js --source-maps",
    "postbuild": "cp src/simpsons-names.json dist/simpsons-names.json",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "commit": "git-cz",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "mocha src/index.test.js -w --compilers js:babel-core/register",
    "test:single": "istanbul cover -x *.test.js _mocha -- -R spec src/index.test.js --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bcabanes/simpsons-names.git"
  },
  "keywords": [
    "random",
    "simspons",
    "charaters"
  ],
  "author": "Benjamin Cabanes <contact@benjamincabanes.com> (http://benjamincabanes.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bcabanes/simpsons-names/issues"
  },
  "homepage": "https://github.com/bcabanes/simpsons-names#readme",
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "6.3.13",
    "chai": "3.4.1",
    "codecov.io": "0.1.6",
    "commitizen": "2.5.0",
    "cz-conventional-changelog": "1.1.5",
    "ghooks": "1.0.3",
    "istanbul": "0.4.2",
    "mocha": "2.3.4",
    "semantic-release": "^4.3.5"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:single && npm run check-coverage"
    }
  }
}
